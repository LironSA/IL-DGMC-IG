    "adm-zip": "^0.5.14",
    "axios": "^1.7.2",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "jsonata": "^2.0.5",
    "path": "^0.12.7",
    "sushi": "^3.0.0",
    "tar": "^7.4.0"
  }
